{
  "name": "skycord",
  "version": "1.2.4",
  "description": "Bluesky Discord bot",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc --build",
    "start": "npm run clean && npm run build && node build/index.js",
    "startpm": "npm run clean && npm run build && pm2 start build/index.js -i -1",
    "startNoClean": "npm run build && node build/index.js",
    "dev": "npm run clean && npm run build && node build/index.js --development",
    "devNoClean": "npm run build && node build/index.js --development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^22.7.7",
    "axios": "^1.7.7",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "mongoose": "^8.7.2",
    "ms": "^2.1.3",
    "pm2": "^5.4.2"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34"
  }
}
